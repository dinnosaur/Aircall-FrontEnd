{
  "name": "@aircall/tractor",
  "version": "1.16.0",
  "description": "UI Component Library for Modern Design",
  "main": "lib/index.js",
  "types": "es/index.d.ts",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "start:storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "release": "standard-version",
    "postrelease": "git push origin HEAD --tags",
    "prebuild": "rimraf es lib",
    "build": "yarn build:es && yarn build:cjs && yarn build:types",
    "build:watch": "concurrently --kill-others \"yarn build:es --watch\" \"yarn build:cjs --watch\" \"yarn build:types --watch\"",
    "build:es": "node .scripts/build es",
    "build:cjs": "node .scripts/build cjs",
    "build:types": "tsc -p tsconfig.build.json",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "generate:icons": "bash .scripts/generate-icons.sh icons",
    "generate:flags": "bash .scripts/generate-icons.sh flags",
    "chromatic": "npx chromatic --build-script-name=build:storybook",
    "component": "hygen component new",
    "test": "jest",
    "test:coverage": "yarn test --coverage"
  },
  "keywords": [],
  "author": "Aircall",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "build",
          "ci",
          "chore",
          "docs",
          "feat",
          "fix",
          "impr",
          "perf",
          "refactor",
          "revert",
          "style",
          "test"
        ]
      ],
      "header-max-length": [
        2,
        "always",
        128
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "jira-prepare-commit-msg"
    }
  },
  "jira-prepare-commit-msg": {
    "messagePattern": "$M [$J]"
  },
  "standard-version": {
    "commitUrlFormat": "{{host}}/{{owner}}/{{repository}}/commits/{{hash}}",
    "releaseCommitMessageFormat": "chore(release): {{currentTag}} [PH-3428] ðŸšœ",
    "compareUrlFormat": "{{host}}/{{owner}}/{{repository}}/branches/compare/{{currentTag}}%0D{{previousTag}}",
    "issuePrefixes": [
      "PH-"
    ],
    "issueUrlFormat": "https://aircall-product.atlassian.net/browse/PH-{{id}}",
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "impr",
        "section": "Improvements"
      },
      {
        "type": "perf",
        "section": "Improvements"
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "hidden": true
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      }
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "./node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "@types/react": ">=16.9.0",
    "@types/react-dom": ">=16.9.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "styled-components": ">=5.1.0"
  },
  "dependencies": {
    "@styled-system/theme-get": "5.1.2",
    "@types/styled-components": "5.1.0",
    "@types/styled-system": "5.1.10",
    "@types/styled-system__theme-get": "5.0.0",
    "framer-motion": "3.2.1",
    "lodash-es": "4.17.21",
    "polished": "4.1.3",
    "pubsub-js": "1.9.3",
    "styled-normalize": "8.0.7",
    "styled-system": "5.1.5",
    "utility-types": "3.10.0"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.8",
    "@babel/helper-regex": "7.10.5",
    "@babel/node": "7.15.8",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "7.15.6",
    "@babel/plugin-transform-modules-commonjs": "7.15.4",
    "@babel/plugin-transform-runtime": "7.10.3",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-docs": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/addon-toolbars": "6.3.12",
    "@storybook/addons": "6.3.12",
    "@storybook/builder-webpack5": "6.3.12",
    "@storybook/manager-webpack5": "6.3.12",
    "@storybook/react": "6.3.12",
    "@svgr/cli": "5.5.0",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/react-hooks": "5.0.3",
    "@testing-library/user-event": "12.7.3",
    "@types/jest": "26.0.20",
    "@types/lodash-es": "4.17.4",
    "@types/node": "14.14.31",
    "@types/pubsub-js": "1.8.2",
    "@types/react": "16.9.35",
    "@types/react-copy-to-clipboard": "4.3.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.3",
    "babel-plugin-styled-components": "1.13.3",
    "chromatic": "5.4.0",
    "concurrently": "5.2.0",
    "eslint": "7.1.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.1.3",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.2.5",
    "hygen": "6.0.4",
    "jest": "26.6.3",
    "jira-prepare-commit-msg": "1.3.3",
    "lint-staged": "10.2.7",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-copy-to-clipboard": "5.0.2",
    "react-docgen-typescript-loader": "3.7.2",
    "react-dom": "16.13.1",
    "standard-version": "8.0.0",
    "styled-components": "5.1.0",
    "ts-jest": "26.5.1",
    "ts-loader": "7.0.5",
    "typescript": "4.0.5",
    "yargs": "15.3.1"
  }
}
